#combine all 9 tables

SELECT category
,subcategory
,name
,current_price
,raw_price
,currency
,discount
,likes_count
,is_new
,brand
,brand_url
,codCountry
,variation_0_color
,variation_1_color
,variation_0_thumbnail
,variation_0_image
,variation_1_thumbnail
,variation_1_image
,image_url
,url
,id
,model
FROM ecommerce.bags 

union all 

